{
  "manifest_version": 3,
  "name": "pocket",
  "version": "1.0.0",
  "description": "포켓: 북마크 관리를 쉽고 간편하게",
  "sidebar_action": {
    "default_page": "index.html",
    "default_icon": {
      "16": "favicon-16x16.png",
      "32": "favicon-32x32.png",
      "96": "favicon-96x96.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["bookmarks", "tabs", "topSites", "storage"]
}
